<h2> Adding the room </h2>

<%= form_for @room do |f| %>

<label>name</label>
<%= f.text_field :name %>
<label>description</label>
<%=f.text_area :description %>
<label>price</label>
<%= f.text_field :price %>
<label>rules</label>
<%= f.text_area :rules%>
<label>address</label>
<%= f.text_area :address %>

<%= f.submit %>
<% end %>