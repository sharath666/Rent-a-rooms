<h2>Rooms </h2>
<table class="table">
<thead>
<tr>
<th> Room Image</th>
<th>Name</th>
<th>Description</th>
<th>price</th>
<th>rules</th>
<th>Address</th>
<th>latitude</th>
<th>longitude</th>

</tr>
</thead>
<tbody>





<% @rooms.each do |room| %>
<tr>
<td><%=image_tag (room.images.url) %></td>
<td><%= link_to room.name, room_path(room.id) %></td>
<td><%=room.description %></td>
<td><%= room.price %></td>
<td><%=room.rules %></td>
<td><%= room.address %></td>
<td><%= room.latitude %></td>
<td><%= room.longitude %></td>
</tr>
<% end %>
</tbody>
</table>